# Environment Configuration for TravelMate Chatbot Application
# Copy this file to .env and fill in your actual API keys and configuration values

# ==============================================================================
# CORE APPLICATION SETTINGS
# ==============================================================================
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_APP_NAME=TravelMate
REACT_APP_VERSION=2.0.0

# ==============================================================================
# LANGFLOW CHATBOT INTEGRATION
# ==============================================================================
# Langflow Server Configuration
REACT_APP_LANGFLOW_BASE_URL=http://localhost:7860
REACT_APP_LANGFLOW_FLOW_ID=your-langflow-flow-id-here
REACT_APP_LANGFLOW_API_KEY=your-langflow-api-key-here

# Langflow Pro Configuration (for premium features)
REACT_APP_LANGFLOW_PRO_FLOW_ID=your-pro-flow-id-here
REACT_APP_LANGFLOW_PRO_API_KEY=your-pro-api-key-here

# ==============================================================================
# GOOGLE MAPS INTEGRATION
# ==============================================================================
# Google Maps JavaScript API
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Google Places API (for location search and details)
REACT_APP_GOOGLE_PLACES_API_KEY=your-google-places-api-key-here

# Google Geocoding API (for address conversion)
REACT_APP_GOOGLE_GEOCODING_API_KEY=your-google-geocoding-api-key-here

# ==============================================================================
# WEATHER API INTEGRATION
# ==============================================================================
# OpenWeatherMap API
REACT_APP_OPENWEATHER_API_KEY=your-openweather-api-key-here
REACT_APP_OPENWEATHER_BASE_URL=https://api.openweathermap.org/data/2.5

# Alternative: AccuWeather API
REACT_APP_ACCUWEATHER_API_KEY=your-accuweather-api-key-here
REACT_APP_ACCUWEATHER_BASE_URL=https://dataservice.accuweather.com

# ==============================================================================
# PAYMENT GATEWAY INTEGRATION
# ==============================================================================
# MoMo Payment Gateway
REACT_APP_MOMO_PARTNER_CODE=your-momo-partner-code-here
REACT_APP_MOMO_ACCESS_KEY=your-momo-access-key-here
REACT_APP_MOMO_SECRET_KEY=your-momo-secret-key-here
REACT_APP_MOMO_ENDPOINT=https://test-payment.momo.vn/v2/gateway/api/create
REACT_APP_MOMO_RETURN_URL=http://localhost:3000/payment/success
REACT_APP_MOMO_NOTIFY_URL=http://localhost:8000/api/payment/momo-webhook

# ZaloPay Payment Gateway
REACT_APP_ZALOPAY_APP_ID=your-zalopay-app-id-here
REACT_APP_ZALOPAY_KEY1=your-zalopay-key1-here
REACT_APP_ZALOPAY_KEY2=your-zalopay-key2-here
REACT_APP_ZALOPAY_ENDPOINT=https://sb-openapi.zalopay.vn/v2/create
REACT_APP_ZALOPAY_RETURN_URL=http://localhost:3000/payment/success
REACT_APP_ZALOPAY_CALLBACK_URL=http://localhost:8000/api/payment/zalopay-webhook

# VNPay Payment Gateway
REACT_APP_VNPAY_TMN_CODE=your-vnpay-tmn-code-here
REACT_APP_VNPAY_HASH_SECRET=your-vnpay-hash-secret-here
REACT_APP_VNPAY_URL=https://sandbox.vnpayment.vn/paymentv2/vpcpay.html
REACT_APP_VNPAY_RETURN_URL=http://localhost:3000/payment/success

# Stripe Payment Gateway (International)
REACT_APP_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key-here
REACT_APP_STRIPE_SECRET_KEY=your-stripe-secret-key-here

# ==============================================================================
# EXTERNAL SERVICES & APIs
# ==============================================================================
# Travel & Booking APIs
REACT_APP_BOOKING_API_KEY=your-booking-com-api-key-here
REACT_APP_AGODA_API_KEY=your-agoda-api-key-here
REACT_APP_EXPEDIA_API_KEY=your-expedia-api-key-here

# Flight APIs
REACT_APP_AMADEUS_API_KEY=your-amadeus-api-key-here
REACT_APP_AMADEUS_SECRET=your-amadeus-secret-here
REACT_APP_SKYSCANNER_API_KEY=your-skyscanner-api-key-here

# Social Media Integration
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id-here
REACT_APP_GOOGLE_CLIENT_ID=your-google-oauth-client-id-here

# ==============================================================================
# AI & MACHINE LEARNING SERVICES
# ==============================================================================
# OpenAI API (for Pro features)
REACT_APP_OPENAI_API_KEY=your-openai-api-key-here
REACT_APP_OPENAI_MODEL=gpt-4
REACT_APP_OPENAI_MAX_TOKENS=2000

# Anthropic Claude API (for Pro features)
REACT_APP_ANTHROPIC_API_KEY=your-anthropic-api-key-here
REACT_APP_ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Google Gemini API (for Pro features)
REACT_APP_GOOGLE_AI_API_KEY=your-google-ai-api-key-here
REACT_APP_GOOGLE_AI_MODEL=gemini-pro

# ==============================================================================
# IMAGE & MEDIA SERVICES
# ==============================================================================
# Unsplash API (for travel images)
REACT_APP_UNSPLASH_ACCESS_KEY=your-unsplash-access-key-here
REACT_APP_UNSPLASH_SECRET_KEY=your-unsplash-secret-key-here

# Pexels API (for travel images)
REACT_APP_PEXELS_API_KEY=your-pexels-api-key-here

# Cloudinary (for image storage and optimization)
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name-here
REACT_APP_CLOUDINARY_API_KEY=your-cloudinary-api-key-here
REACT_APP_CLOUDINARY_API_SECRET=your-cloudinary-api-secret-here

# ==============================================================================
# DATABASE & BACKEND SERVICES
# ==============================================================================
# Backend API Configuration
REACT_APP_BACKEND_API_URL=http://localhost:8000/api
REACT_APP_WEBSOCKET_URL=ws://localhost:8000/ws

# Firebase Configuration (if using Firebase)
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key-here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id-here
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id-here
REACT_APP_FIREBASE_APP_ID=your-firebase-app-id-here

# ==============================================================================
# DEVELOPMENT & DEBUGGING
# ==============================================================================
# Development Mode
REACT_APP_NODE_ENV=development
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your-google-analytics-id-here
REACT_APP_FACEBOOK_PIXEL_ID=your-facebook-pixel-id-here

# ==============================================================================
# SECURITY & AUTHENTICATION
# ==============================================================================
# JWT Configuration
REACT_APP_JWT_SECRET=your-jwt-secret-key-here
REACT_APP_JWT_EXPIRE_TIME=24h

# CORS Configuration
REACT_APP_CORS_ORIGIN=http://localhost:3000

# ==============================================================================
# SUBSCRIPTION & BUSINESS SETTINGS
# ==============================================================================
# Pricing Configuration
REACT_APP_PRO_MONTHLY_PRICE=99000
REACT_APP_PRO_YEARLY_PRICE=990000
REACT_APP_ENTERPRISE_MONTHLY_PRICE=299000
REACT_APP_ENTERPRISE_YEARLY_PRICE=2990000

# Currency
REACT_APP_DEFAULT_CURRENCY=VND
REACT_APP_CURRENCY_SYMBOL=â‚«

# ==============================================================================
# FEATURE FLAGS
# ==============================================================================
REACT_APP_ENABLE_PRO_FEATURES=true
REACT_APP_ENABLE_PAYMENT_GATEWAY=true
REACT_APP_ENABLE_MAP_INTEGRATION=true
REACT_APP_ENABLE_WEATHER_INTEGRATION=true
REACT_APP_ENABLE_BOOKING_INTEGRATION=false
REACT_APP_ENABLE_SOCIAL_LOGIN=false

# ==============================================================================
# INSTRUCTIONS FOR DEVELOPERS
# ==============================================================================
# 1. Copy this file to .env in the same directory
# 2. Fill in your actual API keys and configuration values
# 3. Never commit .env file to version control
# 4. Keep your API keys secure and rotate them regularly
# 5. Use different keys for development, staging, and production environments
